{
  "name": "nuxt-scroll-restoration",
  "version": "2025.05.06",
  "description": "A Nuxt 3 module that restores scroll position during browser back navigation and page reloads",
  "repository": {
    "type": "git",
    "url": "https://github.com/shimizukawa/nuxt-scroll-restoration.git"
  },
  "license": "Apache-2.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "module": "./dist/module.mjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "check": "npm run lint && npm run typecheck",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit --strict",
    "prepack": "npm run check && npm run build",
    "release": "npm run prepack && npm publish"
  },
  "dependencies": {
    "@nuxt/kit": "^3.17.0",
    "@vueuse/core": "^10.11.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^1.3.0",
    "@nuxt/module-builder": "^1.0.1",
    "eslint": "^9.26.0",
    "nuxt": "^3.17.2",
    "typescript": "^5.8.0"
  },
  "peerDependencies": {
    "nuxt": "^3.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "nuxt",
    "nuxt3",
    "scroll",
    "restoration",
    "history",
    "position",
    "spa"
  ]
}
